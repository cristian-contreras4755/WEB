
@{
    ViewData["Title"] = "Sucursal";
    Html.RenderPartial("_RutaNavegacion");
}

@section scripts{

    <script src="~/Admin/assets/plugins/switchery/switchery.min.js"></script>
    <script src="~/Admin/assets/js/Incidencia/Mantenimiento.js"></script>
}


<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
