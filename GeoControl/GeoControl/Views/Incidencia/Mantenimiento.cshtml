                                <h3><i class="fa fa-check-square-o"></i>Estado Incidencia</h3>
                            </div>

                            <div class="card-body">
                                <div class="form-row">
                                    <div class="form-group col-md-12">
                                        <label for="inputEmail4">Descripcion(*)</label>
                                        <input type="text" class="form-control" id="txt_Descripcion_EstInc" placeholder="Estado Incidencia" autocomplete="off">
                                    </div>
                                </div>

                                <div class="form-row">
                                    <button class="btn btn-success" id="btn_GuardarMdfEstInc"><i class="fa fa-floppy-o" aria-hidden="true"></i> Guardar</button>

                                </div>
                                <br />

                                <div class="form-row">
                                    <div class="table-responsive">
                                        <table id="tbl_EstInc" class="table table-bordered table-hover display">
                                            <thead>
                                                <tr>
                                                    <th>cod EstInc</th>
                                                    <th>Descripción</th>
                                                    <th>Opciones</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
